{% extends layout %}
{% block title %}bookmarks{% endblock %}

{% block content %}
{% if entries is empty %}
<div class="messages warning"><p>{{ 'No link available here!'|trans }}</p></div>
{% else %}
    <ul>
    {% for entry in entries %}
        {% include "entry.twig" %}
    {% endfor %}
    </ul>

{% endif %}

{% endblock %}
